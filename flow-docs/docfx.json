{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/includes/media/**"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/*.md"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-MSFlow",
      "contributors_to_exclude": [
        "openpublishingbuild",
        "sudeepku",
        "v-thepet",
        "buck1ey"
      ],
      "breadcrumb_path": "breadcrumb/toc.yml",
      "searchScope": [
        "Flow"
      ],
      "titleSuffix": "Microsoft Flow",
      "search.appverid": "met150"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "flow",
    "markdownEngineName": "markdig"
  }
}
